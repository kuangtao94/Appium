# -*- conding:utf-8 -*-

from Appium_Test.page import tools

pages = tools.paseryaml()

def get_locater(clazz_name,method_name):
    locators = pages[clazz_name]['locators']
    for locator in locators:
        if locator['name'] == method_name:
            return locator


